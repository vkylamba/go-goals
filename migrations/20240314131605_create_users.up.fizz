create_table("users"){
    t.Column("id", "uuid", {"primary": true})
    t.Column("name", "string", {})
    t.Column("email", "string", {})
    t.Column("password_hash", "string", {})
	t.Column("is_superuser", "bool", {null: true})
	t.Column("active", "bool", {null: true})
	t.Column("public", "bool", {null: true})
	t.Timestamps()
}
